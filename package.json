    "@remix-run/react": "*",
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "exifr": "^6.0.0",
    "express": "^4.18.2",
    "intl-parse-accept-language": "^1.0.0",
    "isbot": "^3.6.8",
    "lexical": "^0.6.4",
    "mongodb": "^4.12.1",
    "morgan": "^1.10.0",
    "node-html-parser": "^6.1.5",
